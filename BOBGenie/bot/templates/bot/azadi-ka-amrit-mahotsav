        var thislangvalue = jQuery('.bob-custom-dropdown .bob-custom-dropdown-list .drp_azadi_language li.active').attr('value');
        var thislangtext = jQuery('.bob-custom-dropdown .bob-custom-dropdown-list .drp_azadi_language li.active').text().toLowerCase();
        var thislangclass = thislangtext.replace(/\s/g, "");

        var datavalue = jQuery(this).attr('value');
        var thistext = jQuery(this).text().toLowerCase();
        var thisclass = thistext.replace(/\s/g, '');
        if((datavalue == "" || datavalue == undefined) && (thislangvalue == "" || thislangvalue == undefined)){
          

        }else if(thislangvalue == "" || thislangvalue == undefined){
            jQuery(".azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems[data-product='" + datavalue + "']").addClass(thisclass);
            setTimeout(function(){         
                jQuery(".azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems:not(." + thisclass + ")").remove();
            },200);
        }else if(datavalue == "" || datavalue == undefined){
            jQuery(".azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems[data-language='" + thislangvalue + "']").addClass(thislangclass);
            setTimeout(function(){         
                jQuery(".azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems:not(." + thislangclass + ")").remove();
            },200);
        }else{
            jQuery(".azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems[data-product='" + datavalue + "']").addClass(thisclass);
            jQuery(".azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems[data-language='" + thislangvalue + "']").addClass(thislangclass);
            setTimeout(function(){         
                jQuery(".azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems:not(." + thisclass + "),.azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems:not(." + thislangclass + ")").remove();
            },200);
        }
        setTimeout(function(){         
                jQuery(".azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems").hide();
            },200);
        setTimeout(function(){
            jQuery(".azadi-ka-mahotsav-div.lightbox-video-slider .mediaItems").slice(0, 8).show();
