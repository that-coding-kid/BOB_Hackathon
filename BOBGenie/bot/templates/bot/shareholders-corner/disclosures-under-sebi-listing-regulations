<script>
    (function () {
        var elements = document.querySelectorAll('img[data-src]');

        var index = 0;
        var lazyLoad = function () {
            if (index >= elements.length) return;
            var item = elements[index];
            if ((this.scrollY + this.innerHeight) > item.offsetTop) {
                var src = item.getAttribute("data-src");
                item.src = src;
                item.addEventListener('load', function () {
                    item.removeAttribute('data-src');
                });
                index++;
                lazyLoad();
            }
        };
        var init = function () {
            window.addEventListener('scroll', lazyLoad);
            lazyLoad();
        };
        return init();
    })();




    if (typeof glblCurrentPageID != "undefined" && document.getElementById("whatsappchatanc") != null) {

        /*Set URL */
        var url = "https://wa.me/918433888777?text=Hi";
        if (glblCurrentPageID == "2a2a4af8-e946-4e55-a4da-040502cc49c9" ||
            glblCurrentPageID == "8b8b3479-dd0d-4efb-bc27-201fbd7fefd6" ||
            glblCurrentPageID == "730af99a-d2dd-475b-86cd-7ce14725b1e0" ||
            glblCurrentPageID == "d7ce7630-e51f-4184-ae56-13e3fa9349f4" ||
            glblCurrentPageID == "b9c50d55-3692-4634-b1d2-8dedf94a62ca" ||
            glblCurrentPageID == "2af9e183-338f-4112-8a95-3d4512186dd7" ||
            glblCurrentPageID == "7e8fc1c6-9fd2-4288-b8b8-9ccc1969b090") {
            url = "https://wa.me/7966734416?text=Hi";
        }
        document.getElementById("whatsappchatanc").href = url;

        /*Track Click A/B Testing */
        document.getElementById("whatsappchatanc").addEventListener("click", function () {
            if (typeof this.getAttribute("data-color") != null) {
                window.dataLayer = window.dataLayer || [];
                window.dataLayer.push({
                    'event': 'social_icon',
                    'eventCategory': 'social_icon',
                    'eventAction': 'whatsapp_' + this.getAttribute("data-color")
                });
            }
        });

    }



    document.addEventListener('DOMContentLoaded', function () {
        var links = document.getElementsByTagName("a");
        var i;
        for (i = 0; i < links.length; i++) {
            if (location.hostname !== links[i].hostname && (links[i].href.indexOf("http://") != -1 || links[i].href.indexOf("https://") != -1)) {
                links[i].rel = "nofollow noopener noreferrer";
                links[i].target = "_blank";
            }
        }


        glblFnLoadChatIframeLogic();


    });




    function glblFnLoadChatIframeLogic() {
        const qs = document.querySelectorAll(".bob-msg-li")
        for (const q of qs) {
            var pathname = window.location.pathname;
            if (pathname != '/test-chatbot') {
                q.addEventListener('click', function (e) {
                    if (document.querySelector(".bob-chat-iframe") != null) {
                        document.querySelector(".bob-chat-iframe").classList.add("bob-open-chat-iframe");
                    }
                    if (document.querySelector('#mainDiv iframe') != null) {
                        var actionText = document.getElementById("actionTxt");
                        var actionTextValue = actionText != null ? actionText.value : "";
                        if (actionTextValue != "minimise") {
                            document.querySelector('#mainDiv iframe').setAttribute("src", document.querySelector('#mainDiv iframe').dataset.src);
                        }
                    }
                });
            }
        }

        if (document.querySelector(".bob-chat-iframe") != null) {
            document.querySelector(".bob-chat-iframe").classList.remove("bob-open-chat-iframe");
        }
    }



  /*Att tag in array to remove*/
    var glblHtmlTagList = ["h1", "h2", "h3","h4", "h5", "h6"];
    glblHtmlTagList.forEach(function (tg) {
        var t = document.getElementsByTagName(tg);
        for (i = 0; i < t.length; i++) {
            if (t[i].innerHTML.trim() == "") { t[i].remove() }
        }
    });   

</script>


