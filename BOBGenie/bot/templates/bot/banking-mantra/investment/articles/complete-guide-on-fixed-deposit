<script>
if (glblCurrentLanguage !== "en") {
    var activeItems = document.querySelectorAll(".language-switcher li.active");
    activeItems.forEach(function (item) {
        item.classList.remove("active");
    });

    var languageItems = document.querySelectorAll(".language-switcher li");
    languageItems.forEach(function (item) {
        if (item.getAttribute('data-value') == glblCurrentLanguage && glblCurrentLanguage !== "en") {
            item.classList.add('active');
        }
    });
}

</script>    </div>
