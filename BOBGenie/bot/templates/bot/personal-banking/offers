    }
    function fnCommonFilter(tagType, tagVal) {
        return findValue(Object.values(tagType), tagVal);
    }
    function findValue(arr, key) {
        try {
            var arrValue = arr.find(function (o) { return o.key === key }).value != undefined ? arr.find(function (o) { return o.key === key }).value : "";
            return arrValue;
        }
        catch
        {
        }
    }
    jQuery(".input-group.bob-search-voice-div input[type=text].form-control").on('keyup', function (e) {
        if (jQuery(this).val() == "")
            fnAssignAllFilterCategories();
        if (e.keyCode === 13) {
            var searchedString = jQuery(this).val().toLowerCase();
            fnSearchAndBind(searchedString);
        }
    });
    $(".bob-search-voice-div .input-group-append .offer-search-btn").on('click', function (e) {
        var searchedString = jQuery(".input-group.bob-search-voice-div input[type=text].form-control").val();
        if (searchedString == "")
            fnAssignAllFilterCategories(); //Bind all offers
        else
            fnSearchAndBind(searchedString); //Search and Bind offers
    });
    function fnSearchAndBind(searchedString) {
        var arrUnifiedReqElem = glblJsonDataOffer.map(function (element) {
            return ({
                OfferTitle: element.title,
                ValidTillDate: element.validTillDate,
                IsHandPickedOffer: element.isHandPickedOffer,
                IsExclusiveOffer: element.isExclusiveOffer,
                PromoImage: element.promoImage,
                OfferCategoryKey: element.offerCategoryPhrase,
                PromoHeading: element.promoHeading,
                KnowMoreLink: element.cta,
                ID: element.id,
                OfferViewsCount: element.viewsCount,
                OfferSubmissionDate: element.entryDate,
                ApiSource: element.apiSource,
                ID: element.id
            })
        });

        var offerFilteredBySearch = $.grep(arrUnifiedReqElem, function (element) {
            if (element.OfferTitle.toLowerCase().indexOf(searchedString) != -1) {
                return true;
            }
            else if (element.PromoHeading.toLowerCase().indexOf(searchedString) != -1) {
                return true;
            }
            else if (element.OfferCategoryKey.toLowerCase().indexOf(searchedString) != -1) {
                return true;
            }
            else {
                return false;
            }
        });
        fnBindOffersToPage(offerFilteredBySearch);
        $(".bob-offres-all-card-div .row").html(offerHtmlContent);
        glblFnSetLikedItems();
        fnShareComponentInit();

    }
    function fnGetOfferRemainingTime(date_future) {

        //const offerEndingAt = new Date(date_future);
        //const currentTime = new Date();
        ////const totalSeconds = Math.floor((offerEndingAt - (currentTime)) / 1000);;
        ////const totalMinutes = Math.floor(totalSeconds / 60);
        //const totalHours = Math.floor(totalMinutes / 60);
        //const totalDays = Math.floor(totalHours / 24);
        //const hours = totalHours - (totalDays * 24);
        //const minutes = totalMinutes - (totalDays * 24 * 60) - (hours * 60);
        //const seconds = totalSeconds - (totalDays * 24 * 60 * 60) - (hours * 60 * 60) - (minutes * 60);


        ////return totalDays + "d " + hours + "." + minutes + "." + seconds;// "35d 13.13.07";

        const offerEndingAt = new Date(date_future);
        const currentTime = new Date();
        const diffTime = Math.abs(offerEndingAt - currentTime);
        const totalDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24));
        //console.log(diffTime + " milliseconds");
        //console.log(diffDays + " days");

        return totalDays + " Days Left";
    }

    jQuery(function () {
        var objVoiceToSearch = new fnVoiceToSearch();
        objVoiceToSearch.micButtonElement = ".offer-mic-icon";
        objVoiceToSearch.searchTextBoxElement = ".offer-search-box-input";
        objVoiceToSearch.searchButtonElement = ".offer-search-btn";
        objVoiceToSearch.welcomeAudioId = "welcomeaudio";
        objVoiceToSearch.clickAudioId = "clickaudio";
        objVoiceToSearch.initialise();

    });

</script>



