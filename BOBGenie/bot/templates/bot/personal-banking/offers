<script src="/js/jquerymin.js"></script>
<script src="/js/linq.min.js"></script>
<script>
    var cardOfferFilter = "";
    var arrCustomOfferFilter = [];
    var arrBrandsOfferFilter = [];

    var arrShoppingOfferFilter = [];
    var arrHomeAndLifestyleOfferFilter = [];
    var arrFoodAndBeveragesOfferFilter = [];
    var arrHealthAndInsuranceOfferFilter = [];
    var arrEntertainmentAndServicesOfferFilter = [];
    var arrTravelOfferFilter = [];
    var arrOthersOfferFilter = [];
    var offerHtmlContent = "";
    var offerSortBy = "";

    var loadMoreFlag = true;
    $(".loadmore-btn").on('click', function () {
        $(".bob-loader-parent-div").show(function () {
            loadMoreFlag = false;
            fnAssignAllFilterCategories();
        });

    });

    jQuery('body').on('click', ".bob-tags li a[data-card]", function (e) {
        //Case if URL have parameter cardtype=credit card but clicked debit card tab
        if (fnGetURLParameterValueByName("cardtype") == "credit") {
            FetchOfferJSONData("debit");
        } else {
            fnAssignAllFilterCategories();
        }
    });

    if (jQuery(window).width() > 767) {

        jQuery('body').on('click', "input[name='chkBxHandpicked'],input[name='radiobtn2'],.bob-inner-brands-c-div input[type='checkbox'],.bob-checkboxes-grp-shopping input[type='checkbox'],.bob-checkboxes-grp-home-and-lifestyle input[type='checkbox'],.bob-checkboxes-grp-food-and-beverages input[type='checkbox'],.bob-checkboxes-grp-health-and-insurance input[type='checkbox'],.bob-checkboxes-grp-entertainment-and-services input[type='checkbox'],.bob-checkboxes-grp-travel input[type='checkbox'],.bob-checkboxes-grp-others input[type='checkbox']", function (e) {
            fnAssignAllFilterCategories();
        });

        //To directly use the checkbox change event when on desktop without apply now button.
        $(".chk-liked-by-you input[type=checkbox]").on('change', function () {
            if (this.checked) {
                fnGlblLikedByBindOffer();
            }
            else {
                fnAssignAllFilterCategories();
            }
        });
    }

    function fnGlblLikedByBindOffer() {
        var arrLikedBy = [];
        var finalLikedOfferObject;
        var arrLikedByOfferIds = JSON.parse(fnReadCookie("LikedByItems"));

        //console.log("arrLikedByOfferIds===>" + arrLikedByOfferIds.length);
        //console.log("arrLikedByOfferIds json===>" + JSON.stringify(arrLikedByOfferIds));

        if (arrLikedByOfferIds.length > 0) {
            for (var i = 0; i < arrLikedByOfferIds.length; i++) {

                //console.log("arrLikedByOfferIds ids===>" + arrLikedByOfferIds[i]);
                var resultJSON = glblJsonDataOffer.find(x => x.id == arrLikedByOfferIds[i]);
                if (resultJSON != undefined)
                    arrLikedBy.push(resultJSON);

                //console.log("arrLikedBy inner===>" + JSON.stringify(arrLikedBy));
            }

            //console.log("arrLikedBy===>" + JSON.stringify(arrLikedBy));

            finalLikedOfferObject = Enumerable.from(arrLikedBy)
                .select(function (fieldsVal) {
                    return ({
                        OfferTitle: fieldsVal.title,
                        ValidTillDate: fieldsVal.validTillDate,
                        IsHandPickedOffer: fieldsVal.isHandPickedOffer,
                        IsExclusiveOffer: fieldsVal.isExclusiveOffer,
                        PromoImage: fieldsVal.promoImage,
                        OfferCategoryKey: fieldsVal.offerCategoryPhrase,
                        PromoHeading: fieldsVal.promoHeading,
                        KnowMoreLink: fieldsVal.cta,
                        OfferViewsCount: fieldsVal.viewsCount,
                        OfferSubmissionDate: fieldsVal.entryDate,
                        ApiSource: fieldsVal.apiSource,
                        ID: fieldsVal.id

                    })
                })
                .toArray();

            //console.log("arrLikedByOfferIds new===>" + finalLikedOfferObject.length);
            //console.log("arrLikedByOfferIds len===>" + JSON.stringify(finalLikedOfferObject));

        }


        fnBindOffersToPage(finalLikedOfferObject);
        $(".bob-offres-all-card-div .row").html(offerHtmlContent);
        glblFnSetLikedItems();
        fnShareComponentInit();
    }
    jQuery('body').on('click', "#content .bob-custom-dropdown-list:eq(1) li", function (e) {
        offerSortBy = jQuery(this).data("value");
        fnAssignAllFilterCategories();
    });
    jQuery(function () {
        var cardTypeText = "";
        if (fnGetURLParameterValueByName("cardtype") == "credit") {
            cardTypeText = "credit";
        }
        FetchOfferJSONData(cardTypeText);
    });

    function FetchOfferJSONData(cardType) {


        if (cardType == "credit") {
            jQuery("#offerListingPageHeaderTabs li a.active").removeClass("active");
            jQuery("#offerListingPageHeaderTabs li a[data-card='credit']").addClass("active");
        }

        if (cardType != undefined) {
            cardType = "?cardtype=" + cardType;
        }
        else {
            cardType = "";
        }

        jQuery.getJSON("/handlers/bob/countrywebsites/india/get-offers-json-data.ashx" + cardType, function (data) {
            glblJsonDataOffer = data;
            jQuery(".bob-loader-parent-div").hide();
            fnAssignAllFilterCategories();
            setTimeout(function () {
                jQuery(".bob-offer-top-filter-div .bob-custom-dropdown-list li").each(function () {
                    if (jQuery(this).data("value") == "exclusiveoffer") {
                        jQuery(this).click();
                    }
                });
            }, 100);
        });
    }

    jQuery(".btn-clear-all").on("click", function () {
        $('.form-check-input:checked').prop('checked', false);
        fnAssignAllFilterCategories();
        jQuery(".closed-pp").click();
    });
    // Mobile Apply now changes
    if (jQuery(window).width() <= 767) {
        jQuery(".mob-apply-btn").on("click", function () {
            fnAssignAllFilterCategories();
            if ($(".chk-liked-by-you input[type=checkbox]").is(':checked'))
                fnGlblLikedByBindOffer();
            //to close
            jQuery(".closed-pp").click();
        });
        jQuery('body').on('click', "#mCSB_1_container li", function (e) {
            //console.log("mCSB_1_container===>"+jQuery(this).data("value"));
            offerSortBy = jQuery(this).data("value");
            fnAssignAllFilterCategories();
        });
    }
    // Mobile Apply now changes


    function fnAssignAllFilterCategories() {
        arrCustomOfferFilter = [];
        arrBrandsOfferFilter = [];
        arrShoppingOfferFilter = [];
        arrHomeAndLifestyleOfferFilter = [];
        arrFoodAndBeveragesOfferFilter = [];
        arrHealthAndInsuranceOfferFilter = [];
        arrEntertainmentAndServicesOfferFilter = [];
        arrTravelOfferFilter = [];
        arrOthersOfferFilter = [];

        cardOfferFilter = $(".bob-tags li a.active").data('card');

        $(".bob-inner-custom-c-div input:checked").each(function () {
            if ($(this).val() != "Liked by you")
                arrCustomOfferFilter.push($(this).val());
        });

        //customOfferFilter = $("input[name='radiobtn2']").is(':checked') ? $("input[name='radiobtn2']:checked").val() : "";

        $(".bob-inner-brands-c-div input:checked").each(function () {
            arrBrandsOfferFilter.push($(this).val());
        });
        $(".bob-checkboxes-grp-shopping input:checked").each(function () {
            arrShoppingOfferFilter.push($(this).val());
        });
        $(".bob-checkboxes-grp-home-and-lifestyle input:checked").each(function () {
            arrHomeAndLifestyleOfferFilter.push($(this).val());
        });
        $(".bob-checkboxes-grp-food-and-beverages input:checked").each(function () {
            arrFoodAndBeveragesOfferFilter.push($(this).val());
        });
        $(".bob-checkboxes-grp-health-and-insurance input:checked").each(function () {
            arrHealthAndInsuranceOfferFilter.push($(this).val());
        });
        $(".bob-checkboxes-grp-entertainment-and-services input:checked").each(function () {
            arrEntertainmentAndServicesOfferFilter.push($(this).val());
        });
        $(".bob-checkboxes-grp-travel input:checked").each(function () {
            arrTravelOfferFilter.push($(this).val());
        });
        $(".bob-checkboxes-grp-others input:checked").each(function () {
            arrOthersOfferFilter.push($(this).val());
        });
        fnApplyFilter();
    }
    function fnApplyFilter() {


        var arrFinalOfferJSON = Enumerable.from(glblJsonDataOffer)
            .where(function (aa) { return cardOfferFilter != "All Card" && cardOfferFilter != "credit" ? fnCommonFilter(aa.paymentTypeTags, cardOfferFilter) : true })
            //.where(function (aa) { return customOfferFilter != "" ? fnCommonFilter(aa.customFiltersTag, customOfferFilter) : true })
            .where(function (aa) { return arrCustomOfferFilter.length > 0 ? arrCustomOfferFilter.filter(function (elem) { return fnCommonFilter(aa.customFiltersTag, elem) }).toString() : true })
            .where(function (aa) { return arrShoppingOfferFilter.length > 0 ? arrShoppingOfferFilter.filter(function (elem) { return fnCommonFilter(aa.shoppingTag, elem) }).toString() : true })
            .where(function (aa) { return arrHomeAndLifestyleOfferFilter.length > 0 ? arrHomeAndLifestyleOfferFilter.filter(function (elem) { return fnCommonFilter(aa.homeAndLifestyleTag, elem) }).toString() : true })
            .where(function (aa) { return arrFoodAndBeveragesOfferFilter.length > 0 ? arrFoodAndBeveragesOfferFilter.filter(function (elem) { return fnCommonFilter(aa.foodAndBeveragesTag, elem) }).toString() : true })
            .where(function (aa) { return arrHealthAndInsuranceOfferFilter.length > 0 ? arrHealthAndInsuranceOfferFilter.filter(function (elem) { return fnCommonFilter(aa.healthAndInsuranceTag, elem) }).toString() : true })
            .where(function (aa) { return arrEntertainmentAndServicesOfferFilter.length > 0 ? arrEntertainmentAndServicesOfferFilter.filter(function (elem) { return fnCommonFilter(aa.entertainmentAndServicesTag, elem) }).toString() : true })
            .where(function (aa) { return arrTravelOfferFilter.length > 0 ? arrTravelOfferFilter.filter(function (elem) { return fnCommonFilter(aa.travelTags, elem) }).toString() : true })
            .where(function (aa) { return arrOthersOfferFilter.length > 0 ? arrOthersOfferFilter.filter(function (elem) { return fnCommonFilter(aa.othersTag, elem) }).toString() : true })
            .where(function (aa) { return arrBrandsOfferFilter.length > 0 ? arrBrandsOfferFilter.filter(function (elem) { return fnCommonFilter(aa.brandTags, elem) }).toString() : true })

            .select(function (fieldsVal) {
                return ({
                    OfferTitle: fieldsVal.title,
                    ValidTillDate: fieldsVal.validTillDate,
                    IsHandPickedOffer: fieldsVal.isHandPickedOffer,
                    IsExclusiveOffer: fieldsVal.isExclusiveOffer,
                    PromoImage: fieldsVal.promoImage,
                    OfferCategoryKey: fieldsVal.offerCategoryPhrase,
                    PromoHeading: fieldsVal.promoHeading,
                    KnowMoreLink: fieldsVal.cta,
                    OfferViewsCount: fieldsVal.viewsCount,
                    OfferSubmissionDate: fieldsVal.entryDate,
                    ID: fieldsVal.id,
                    ApiSource: fieldsVal.apiSource,
                    entryDate: fieldsVal.entryDate

                })
            })
            .toArray();



        if (offerSortBy != "" && offerSortBy != undefined) {
            //console.log("offerSortBy==" + offerSortBy);
            //alert("offerSortBy===" + offerSortBy);
            if (offerSortBy === "latest")
                fnSortArrayWithOrder(arrFinalOfferJSON, 'entryDate', false);
            else if (offerSortBy === "mostviewed")
                fnSortArrayWithOrder(arrFinalOfferJSON, 'OfferViewsCount', false);
            else if (offerSortBy === "expiringsoon")
                fnSortArrayWithOrder(arrFinalOfferJSON, 'ValidTillDate', true);
            else if (offerSortBy === "oldest")
                fnSortArrayWithOrder(arrFinalOfferJSON, 'entryDate', true);
            else if (offerSortBy === "exclusiveoffer")
                fnSortArrayWithOrder(arrFinalOfferJSON, 'IsExclusiveOffer', false);

        }
        else {
            fnSortArrayWithOrder(arrFinalOfferJSON, 'entryDate', false);
        }

        if ($("input[name='chkBxHandpicked']").is(':checked')) {
            fnSortArrayWithOrder(arrFinalOfferJSON, 'IsHandPickedOffer', false);
        }

        fnBindOffersToPage(arrFinalOfferJSON);
        $(".bob-offres-all-card-div .row").html(offerHtmlContent);
        glblFnSetLikedItems();
        fnShareComponentInit();
    }
    function fnSortArrayWithOrder(jsonObject, keyToSortBy, orderByAsc) {
        function fnSortByKey(a, b) {
            var x = a[keyToSortBy];
            var y = b[keyToSortBy];

            if (orderByAsc) //Asc
                return ((x < y) ? -1 : ((x > y) ? 1 : 0));
            else //Desc
                return ((y < x) ? -1 : ((y > x) ? 1 : 0));
        }
        jsonObject.sort(fnSortByKey);
    }
    var lengthForLoadMore = 0;
    function fnBindOffersToPage(arrFinalOfferJSON) {
        if (typeof arrFinalOfferJSON != "undefined" && arrFinalOfferJSON.length > 0) {
            offerHtmlContent = "";
            lengthForLoadMore = lengthForLoadMore + 12;
            if (jQuery(".offer-search-box-input").val() != "") {
                lengthForLoadMore = arrFinalOfferJSON.length;
            }


            for (var index = 0; index < lengthForLoadMore; index++) {

                var imageOrHeading = "";
                if (arrFinalOfferJSON[index].ApiSource == "mastercard" && arrFinalOfferJSON[index] != undefined) {
                    imageOrHeading = "<h3>" + arrFinalOfferJSON[index].OfferTitle + "</h2>";
                } else {
                    if (arrFinalOfferJSON[index].PromoImage != "") {
                        imageOrHeading = "<img src='" + arrFinalOfferJSON[index].PromoImage + "' alt='offer' class='img-fluid'>";
                    }
                    else {
                        imageOrHeading = "<h3>" + arrFinalOfferJSON[index].OfferTitle + "</h2>";
                    }
                }

                offerHtmlContent += getOfferTemplate().replace("$$promoImage$$", imageOrHeading)
                    .replace("$$validTillDate$$", fnGetOfferRemainingTime(arrFinalOfferJSON[index].ValidTillDate))
                    .replace("$$offerCategoryKey$$", arrFinalOfferJSON[index].OfferCategoryKey)
                    .replace("$$promoHeading$$", arrFinalOfferJSON[index].PromoHeading)
                    //.replace("$$cta$$", arrFinalOfferJSON[index].KnowMoreLink)
                    .replace(/#cta#/g, arrFinalOfferJSON[index].KnowMoreLink)
                    .replace("$$viewsCount$$", arrFinalOfferJSON[index].OfferViewsCount)
                    .replace("$$ID$$", arrFinalOfferJSON[index].ID);
            }

            if (lengthForLoadMore >= arrFinalOfferJSON.length) {
                $(".loadmore-btn").hide();
            } else {
                $(".bob-loader-parent-div").hide();

                $(".loadmore-btn").show();
            }
        }
        else {
            offerHtmlContent = "No Offers Found!";
            $(".loadmore-btn").hide();
        }

    }
    function getOfferTemplate() {
        var offerTemplate = "";
        offerTemplate += "<div class='col-sm-4 share-parent'>";
        offerTemplate += "<div class='bob-offer-card-div'>";
        offerTemplate += "<div class='bob-offer-logo-time-div'>";
        offerTemplate += "<a href='#cta#'>$$promoImage$$</a>";
        offerTemplate += "<span>";
        offerTemplate += "<svg xmlns='http://www.w3.org/2000/svg' width='13.593' height='13.593' viewBox='0 0 13.593 13.593' fill='#a8a8a8'><path class='a' d='M6.8,0a6.8,6.8,0,1,0,6.8,6.8A6.8,6.8,0,0,0,6.8,0Zm0,12.161A5.365,5.365,0,1,1,12.161,6.8,5.371,5.371,0,0,1,6.8,12.161Z' /><path class='a' d='M60.942,42.353H58.155V39.94a.716.716,0,1,0-1.431,0v3.129a.716.716,0,0,0,.716.716h3.5a.716.716,0,0,0,0-1.431Z' transform='translate(-51.311 -35.481)' /></svg>";
        offerTemplate += "$$validTillDate$$";
        offerTemplate += "</span>";
        offerTemplate += "</div>";
        offerTemplate += "<h5 class='bob-offer-cat'>$$offerCategoryKey$$</h5>";
        offerTemplate += "<a href='#cta#'><h2 class= 'share-title'>$$promoHeading$$</h2></a>";
        offerTemplate += "<div class='bob-offer-card-btn-div'>";
        offerTemplate += "<a href='#cta#' class='bob-arrow-button share-url'>";
        offerTemplate += "Know More";
        offerTemplate += '<svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 512 512" style="enable-background:new 0 0 512 512;" xml:space="preserve"><path d="M508.875,248.458l-160-160c-4.167-4.167-10.917-4.167-15.083,0c-4.167,4.167-4.167,10.917,0,15.083l141.792,141.792H10.667C4.771,245.333,0,250.104,0,256s4.771,10.667,10.667,10.667h464.917L333.792,408.458c-4.167,4.167-4.167,10.917,0,15.083c2.083,2.083,4.813,3.125,7.542,3.125c2.729,0,5.458-1.042,7.542-3.125l160-160C513.042,259.375,513.042,252.625,508.875,248.458z" /></svg>';
        offerTemplate += "</a>";
        offerTemplate += "<ul class='bob-offer-views-ul'>";
        offerTemplate += "<li>";
        offerTemplate += "$$viewsCount$$ Views ";
        offerTemplate += "<svg xmlns='http://www.w3.org/2000/svg' width='13.965' height='9.819' viewBox='0 0 13.965 9.819' fill='#707070'><path class='a' d='M12.915,80.527a8.043,8.043,0,0,0-6.93-4.676c-3.5,0-6.008,2.734-6.93,4.676a.546.546,0,0,0,0,.468,8.043,8.043,0,0,0,6.93,4.676c3.5,0,6.008-2.734,6.93-4.676A.546.546,0,0,0,12.915,80.527Zm-6.93,4.053A6.889,6.889,0,0,1,.166,80.761a6.892,6.892,0,0,1,5.819-3.819A6.889,6.889,0,0,1,11.8,80.761,6.892,6.892,0,0,1,5.985,84.579Zm0-6.546a2.728,2.728,0,1,0,2.728,2.728A2.731,2.731,0,0,0,5.985,78.033Zm0,4.364a1.637,1.637,0,1,1,1.637-1.637A1.638,1.638,0,0,1,5.985,82.4Z' transform='translate(0.998 -75.851)' /></svg>";
        offerTemplate += "</li>";
        offerTemplate += "<li class='likes-li' data-id='$$ID$$'>";
        offerTemplate += '<svg xmlns="http://www.w3.org/2000/svg" width="13.86" height="11.535" viewBox="0 0 13.86 11.535" fill="#707070" class="unlike-svg"><g transform="translate(0 0)"><path class="a" d="M12.746,6.828a3.87,3.87,0,0,0-5.466,0l-.35.35-.35-.35a3.869,3.869,0,0,0-5.466,0,3.91,3.91,0,0,0-.022,5.444C2.427,13.755,6.364,16.96,6.531,17.1a.609.609,0,0,0,.386.137H6.93a.59.59,0,0,0,.4-.137c.167-.136,4.1-3.341,5.44-4.824A3.91,3.91,0,0,0,12.746,6.828Zm-.887,4.625A65.24,65.24,0,0,1,6.93,15.84,65.1,65.1,0,0,1,2,11.453a2.7,2.7,0,0,1-.022-3.76,2.644,2.644,0,0,1,3.736,0l.769.769a.592.592,0,0,0,.328.165.613.613,0,0,0,.563-.165l.77-.77a2.645,2.645,0,0,1,3.736,0A2.7,2.7,0,0,1,11.859,11.452Z" transform="translate(0 -5.697)" /></g></svg>';
