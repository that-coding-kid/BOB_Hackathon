<script>
var mediaData = jQuery('.bob-press-div .container .row:first-child').html();
jQuery(document).on('click', '.go-btn-news', function () {
    jQuery('.bob-press-div .container .row:first-child .mediaItems').remove();
    jQuery('.bob-press-div .container .row:first-child').append(mediaData);

    var dataMonth="", dataYear = "";
    if (jQuery("#drp_media_years li.active").attr("value") != undefined) {
            dataYear = jQuery.trim(
                jQuery("#drp_media_years li.active").attr("value").toLowerCase()
            );
        }
    if (jQuery("#drp_media_months li.active").attr("value") != undefined) {
        dataMonth = jQuery.trim(
            jQuery("#drp_media_months li.active").attr("value").toLowerCase()
        );
    }

    setTimeout(function () {
        if (dataYear != null && dataYear != "" && dataMonth != null && dataMonth != "") {
            jQuery('.bob-press-div .row .mediaItems[data-year][data-month]:not([data-year="' + dataYear + '"][data-month="' + dataMonth + '"])').remove();
        } else if (dataYear != null && dataYear != "" && (dataMonth == null || dataMonth == "")) {
            jQuery('.bob-press-div .row .mediaItems[data-year]:not([data-year="' + dataYear + '"])').remove();
        } else if ((dataYear == null || dataYear == "") && dataMonth != null && dataMonth != "") {
            jQuery('.bob-press-div .row .mediaItems[data-month]:not([data-month="' + dataMonth + '"])').remove();
        }
        for (var i = 1; i <= 8; i++){
            jQuery('.bob-press-div .row .mediaItems:nth-child(' + i +')').css('display', 'block');
        }
        jQuery("html, body").animate({
            scrollTop: 0
        }, 300);
        jQuery('.bob-report-load-more').fadeIn()
    });
});
jQuery(document).on('click', '.reset-btn-news', function () {
    jQuery('.bob-press-div .container .row:first-child .mediaItems').remove();
    jQuery('.bob-press-div .container .row:first-child').append(mediaData);
    for (var i = 1; i <= 8; i++) {
        jQuery('.bob-press-div .row .mediaItems:nth-child(' + i + ')').css('display', 'block');
    }
    jQuery("html, body").animate({
        scrollTop: 0
    }, 300);
    jQuery('.bob-report-load-more').fadeIn();
});
</script>    <!-- /#wrapper -->
</body>
