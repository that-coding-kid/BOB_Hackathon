            //     console.log('start  ' + startval);
            //     console.log('end  ' + endval);

            // };

            // }
            element.onmouseover = function (e) {
                // clearTimeout(carouselTimeout);


            };

        element.onmouseout = function (e) {
            // currentSlide(index);
        };

        });

        


        document.querySelectorAll('script-slider .sxa-bullets').forEach((e, index) => {
            e.onclick = function () {
                clearTimeout(carouselTimeout);
                currentSlide(index);
            }
        });

        function showSlides(n) {
            if (document.getElementsByClassName("script-slider").length > 0) {
                var i;
        var slides = document.getElementsByClassName("slide");
        var dots = document.getElementsByClassName("sxa-bullets");
                if (n > slides.length) {slideIndex = 0}
        if (n < 0) {slideIndex = slides.length}
        for (i = 0; i < slides.length; i++) {
            slides[i].style.display = "none";
        if (slides[i].className.indexOf("fade") == -1) {
            slides[i].className += " fade";
                    }
                }
        for (i = 0; i < dots.length; i++) {
            dots[i].className = dots[i].className.replace(" active", "");
                }
        slideIndex++;
                if (slideIndex > slides.length) {slideIndex = 1}

        slides[slideIndex - 1].style.display = "block";
        dots[slideIndex - 1].className += " active";

        carouselTimeout = setTimeout(showSlides, 6000);
            }
        }


        /*Carousel Code end*/



        if (document.getElementsByTagName("html").length > 0) {
            document.getElementsByTagName("html")[0].classList.remove("no-js");
        }
        if (document.querySelector(".road-block-popup .popupCloseBtn") != null) {
            document.querySelector(".road-block-popup .popupCloseBtn").addEventListener("click", function () {
                var appBanners = document.getElementsByClassName('road-block-popup');
                for (var i = 0; i < appBanners.length; i++) {
                    appBanners[i].style.display = 'none';
                }
            });
        }

        function loadTopNavBarInMobile() {
            if (window.width < 1025) {
                if (typeof jQuery == "undefined") {
                    const remove = (sel) => document.querySelectorAll(sel).forEach(el => el.remove());

        var login_drop =
        '<li><div class="bob-custom-dropdown bob-login-dropdown bob-logincc-div">' +
            document.querySelector(".bob-login-dropdown").innerHTML +
            "</div></li>";
        remove(".bob-login-dropdown");

        document.querySelector(".bob-top-bottom-navbar .bob-bottom-second-ul li:last-child").insertAdjacentHTML("afterend", login_drop);

        var setting_html = '<div class="bob-top-navbar"><ul class="bob-nav-right-link">' + document.querySelector(".bob-nav-right-link").innerHTML +
            "</ul></div>";
        remove(".bob-nav-right-link");
        document.querySelector(".bob-mobile-sidebar .bob-navbar").insertAdjacentHTML("beforeend", setting_html);

        var main_links =
        '<ul class="bob-bottom-first-ul">' + document.querySelector(".bob-bottom-first-ul").innerHTML + "</ul>";
        remove(".bob-bottom-first-ul");
        document.querySelector(".bob-mobile-sidebar .bob-top-bottom-navbar").insertAdjacentHTML("beforeend", main_links);

        var middle_links =
        '<ul class="bob-middle-first-link bob-middle-second-link">' +
            document.querySelector(".bob-middle-first-link.bob-middle-second-link").innerHTML +
            "</ul>";
        remove(".bob-middle-first-link.bob-middle-second-link");
        document.querySelector(".bob-mobile-sidebar .bob-middle-navbar").insertAdjacentHTML("beforeend", middle_links);


        var mndl = document.querySelectorAll(".bob-nav-inner-list .bob-simple-btn");
        mndl.forEach(function (elem) {
            console.log(elem.closest(".main-nav-ddl-list"))
                        elem.closest(".main-nav-ddl-list").classList.remove("nav-no-button");
                    });

        if (document.querySelector(".lang_li") != null) {
            document.querySelector(".lang_li").addEventListener("click", function (ele) {
                if (document.getElementsByClassName("language-switcher")[0].style.display == "none") {
                    document.getElementsByClassName("language-switcher")[0].style.display = "block";
                } else {
                    document.getElementsByClassName("language-switcher")[0].style.display = "none";
                }
            });
                    }


        var loginddl = document.getElementById("header").querySelectorAll(".bob-login-dropdown");
        var logindd = document.getElementById("header").querySelectorAll(".bob-login-dropdown");
                    if (logindd.length > 0) {
            loginddl[0].addEventListener("click", function (ele) {
                ele.target.classList.add("bob-open-dropdown");
                document.body.classList.add("bob-cust-drop-body-open");
                document.querySelector(".bob-custom-drop-popup-div").classList.add("active-drop")
            });
                    }


        AddEventAndClassOnElementAndBody(".bob-mobile-bar", "bob-active-bar", "bob-open-nav-mob");
        AddEventAndClassOnElementAndBody(".notification_svg_h", "", "notification-open");
        AddEventAndClassOnElementAndBody(".search-popup", "", "bob-open-serach-popup");


        function AddEventAndClassOnElementAndBody(elem, calssNameToAddOnEle, classNameToAddOnBody) {
                        var qs = document.querySelectorAll(elem);
                        if (qs.length > 0) {
            qs.forEach(function (e) {
                e.addEventListener("click", function () {
                    if (calssNameToAddOnEle != "") {
                        if (e.classList.contains(calssNameToAddOnEle)) {
                            e.classList.remove(calssNameToAddOnEle);
                            fnCloseAllDropdownFromMobileMenu(elem);
                        } else {
                            e.classList.add(calssNameToAddOnEle);
                        }
                    }
                    if (classNameToAddOnBody != '') {
                        if (document.body.classList.contains(classNameToAddOnBody)) {
                            document.body.classList.remove(classNameToAddOnBody);
                        }
                        else {
                            document.body.classList.add(classNameToAddOnBody);
                        }
                    };
                });
            });
                        }
                    }
                }
            }
        }

        document.addEventListener('DOMContentLoaded', function () {
            loadTopNavBarInMobile();
        });


        function fnCloseAllDropdownFromMobileMenu(elem) {
            if (elem == ".bob-mobile-bar") {
                const element = document.querySelector(".bob-mobile-bar");
        if (element != null) {
                    if (!element.classList.contains("bob-active-bar")) {
                        var mobSB = document.querySelectorAll(".bob-mobile-sidebar .bob-open-dropdown");
                        if (mobSB.length > 0)
        mobSB.forEach(function (e) {
            e.classList.remove("bob-open-dropdown");
                            });
                    }
                }
            }
        }

    </script>





