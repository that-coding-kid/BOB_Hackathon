        /*Carousel Code start*/
        let slideIndex = 0;
        var carouselTimeout;
        showSlides(slideIndex);


        /*Fnction Bullet point click*/
        function plusSlides(n) {
            showSlides(slideIndex += n);
        }

        /*Fnction go to current slide*/
        function currentSlide(n) {
            showSlides(slideIndex = n);
        }


        /*Stop slide onmouse hover and start on mouseout*/
        var windowWidth = screen.Width;
        console.log(windowWidth)
        const carousel = document.querySelectorAll('script-slider .slides .slide');
        var startval = 0, endval = 0, direction;
        var list = [...document.querySelectorAll('.sxa-bullets')];
        carousel.forEach(function (element, index) {
            // if (windowWidth > 768) {
            console.log('if')
                element.ondragstart = function (e) {
            startval = e.clientX;
                };

        element.ondragend = function (e) {
            endval = e.clientX;
                    if (startval > endval) {
            direction = "left";
        var active = document.querySelector('.sxa-bullets.active');
        var activeindex = list.indexOf(active) + 2;
        document.querySelector('.sxa-bullets:nth-child(' + activeindex + ')').click();
                    } else {
            direction = "right";
        var active = document.querySelector('.sxa-bullets.active');
        var activeindex = list.indexOf(active);
        document.querySelector('.sxa-bullets:nth-child(' + activeindex + ')').click();
                    }
        console.log(activeindex);
                };

            // }
            // else {

            // console.log('else')

