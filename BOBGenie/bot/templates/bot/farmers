    <script>
        document.getElementsByTagName("body")[0].style.opacity = "1";

        var allJS = ['/js/jquerymin.js', '/js/jquerymCustomScrollbar.js', '/-/media/themes/bob/india/india/scripts/optimized-min.js?t=20150515T071943:635672711833756301Z',];

        function loadJavaScript() {
          //  $xa = $;
           

            var body = document.body;
            if (body.classList.toString().indexOf("jsLoaded") == -1) {
                body.classList.add("jsLoaded");

                if (allJS.length > 0) {
                    loadScript(allJS, function () {
                        console.log('loaded');
                        fnCreateCookie("jsloaded", "1", 525600);
                    });
                }

            }
        }



    //If window is scrolled
    document.getElementsByTagName('body')[0].onscroll = function () {
      loadJavaScript();
    };

    //If window view is 1 px down
    if (window.scrollY > 0) {
        loadJavaScript();
    }


    //If mouse is moved
        document.body.addEventListener('mouseenter', loadJavaScript, true);

        function loadScript(arrayOfUrlStrings, callback) {
            var numScripts = arrayOfUrlStrings.length;
            var count = 0;
            var headElement = document.getElementsByTagName('head')[0]

            function onLoad() {
                count += 1;

                if (count === numScripts) {
                    callback();
                } else {
                    addScript();
                }
            }

            function addScript() {
                var script = document.createElement('script');
                script.src = arrayOfUrlStrings[count];
                script.onload = onLoad;
                headElement.appendChild(script);
            }

            addScript();
        }

    </script>

    <script id="scJS" src=""></script>



    <!-- /#wrapper -->



    <script>
        var allPageSpecificCSS = [];
        var currentTemplateName = "page";
        if (currentTemplateName == "home") {
            currentTemplateName = "home-page";
        }

        //Default device
        var deviceType = "desktop";
        var width = document.getElementById('wrapper').clientWidth;
        if (width < 767) {
            deviceType = "mobile";
        }
        else {
            deviceType = "desktop";
        }

        console.log("Device=" + deviceType);
        for (var i = 0; i < allPageSpecificCSS.length; i++) {
            if (allPageSpecificCSS[i] != "" && allPageSpecificCSS[i].indexOf(currentTemplateName+"-" + deviceType) != -1) {
                loadCSS(allPageSpecificCSS[i]);
            }
        }




        (function () {
            var elements = document.querySelectorAll('img[data-src]');

            var index = 0;
            var lazyLoad = function () {
                if (index >= elements.length) return;
                var item = elements[index];
                if ((this.scrollY + this.innerHeight) > item.offsetTop) {
                    var src = item.getAttribute("data-src");
                    item.src = src;
                    item.addEventListener('load', function () {
                        item.removeAttribute('data-src');
                    });
                    index++;
                    lazyLoad();
                }
            };
            var init = function () {
                window.addEventListener('scroll', lazyLoad);
                lazyLoad();
            };
            return init();
        })();





        if (!String.prototype.startsWith) {
            Object.defineProperty(String.prototype, 'startsWith', {
                value: function (search, rawPos) {
                    var pos = rawPos > 0 ? rawPos | 0 : 0;
                    return this.substring(pos, pos + search.length) === search;
                }
            });
        }

        /*FUNCTION TO CREATE COOKIE*/
        function fnCreateCookie(cookiname, cookivalue, timeinminutes) {
            var d = new Date();
            d.setTime(d.getTime() + (timeinminutes * 60 * 1000));
            var expires = "expires=" + d.toGMTString();
            document.cookie = cookiname + "=" + cookivalue + ";" + expires + ";path=/";
        }

        /*FUNCTION TO READ COOKIE VALUE BY NAME*/
        function fnReadCookie(cookiname) {
            var name = cookiname + "=";
            var decodedCookie = decodeURIComponent(document.cookie);
            var ca = decodedCookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        /*FUNCTION TO DELETE ALL CREATED COOKIES*/
        function fnDeleteAllCookie() {
            var arrCookies = document.cookie.split(";");
            for (var i = 0; i < arrCookies.length; i++) {
                var c = arrCookies[i].split("=");
                deleteCookie(c[0]); /*Calling the below delete function*/
            }
        }

        /*FUNCTION TO DELETE  COOKIE BY NAME*/
        function fnDeleteCookie(cookiname) {
            var d = new Date();
            d.setDate(d.getDate() - 1);
            var expires = ";expires=" + d;
            var value = "";
            document.cookie = cookiname + "=" + value + expires + "; path=/";
        }

        function fnGetURLParameterValueByName(parName) {
            parName = parName.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
            var regex = new RegExp("[\\?&]" + parName + "=([^&#]*)");
            var results = regex.exec(location.search);
            return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        }

        function glblFnRemoveElementsByClass(className) {
            const elements = document.getElementsByClassName(className);
            while (elements.length > 0) {
                elements[0].parentNode.removeChild(elements[0]);
            }
        }


        function fnRemoveCookieWarning() {
            if (fnReadCookie("privacy-notification") == "1") {
                glblFnRemoveElementsByClass("privacy-warning");
            }
        }


        /*Remove cookie warning if no cookie set*/
        const pwel = document.getElementsByClassName('privacy-warning')[0];
        if (typeof pwel != "undefined") {
            const pweAnc = pwel.getElementsByTagName('a')[0];
            if (typeof pweAnc != "undefined") {
                pweAnc.setAttribute('onclick', 'fnCreateCookie("privacy-notification", "1", 525600); fnRemoveCookieWarning();')
            }
        }

        fnRemoveCookieWarning();

        if (fnReadCookie("jsloaded") != "") {
            loadJavaScript();
        }


    </script>


</body>
